{
  "name": "icatalyst-nestjs-starter",
  "version": "1.0.0",
  "description": "A NestJS starter template for ICatalyst projects",
  "author": "Christopher Zhong",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.ts",
    "prepare": "node --eval \"try { require('husky').install() } catch (error) { if (error.code !== 'MODULE_NOT_FOUND') throw error }\""
  },
  "engines": {
    "node": "^16.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^10.0.4",
    "@nestjs/core": "^10.0.4",
    "@nestjs/platform-express": "^10.0.4",
    "@nestjs/swagger": "^7.0.12",
    "@nestjs/terminus": "^10.0.1",
    "@prisma/client": "^5.0.0",
    "ajv": "^8.12.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "prisma": "^5.0.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.1.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.4",
    "@semantic-release/gitlab-config": "^12.0.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.17.19",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "semantic-release": "^21.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
