{
  "name": "icatalyst-nestjs-starter",
  "version": "1.0.0",
  "description": "A NestJS starter template for ICatalyst projects",
  "author": "Christopher Zhong",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.ts",
    "prepare": "node --eval \"try { require('husky').install() } catch (error) { if (error.code !== 'MODULE_NOT_FOUND') throw error }\""
  },
  "engines": {
    "node": ">=16 <17"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@prisma/client": "^4.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "helmet": "^6.0.1",
    "prisma": "^4.8.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@semantic-release/gitlab-config": "^10.0.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^16.18.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "semantic-release": "^19.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
